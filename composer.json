{
    "name": "webfactory/yii2_advanced_api",
    "description": "Advanced API system using Yii2",
    "keywords": ["yii2", "WebFactory", "advanced", "project template"],
    "homepage": "http://www.advanced.domain.com/",
    "type": "project",
    "license": "Apache License",
    "support": {
        "issues": "https://github.com/webfactorycuba/yii2_advanced_api/issues?state=open",
        "wiki": "https://github.com/webfactorycuba/yii2_advanced_api/wiki/",
        "source": "https://github.com/webfactorycuba/yii2_advanced_api"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.28",
        "yiisoft/yii2": "~2.0.35",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "mdmsoft/yii2-admin": "^2.0@dev",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-export": "^1.3",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-number": "*",
        "hyperia/yii2-secure-headers": "^1.0",
        "2amigos/yii2-translateable-behavior": "*",
        "machour/yii2-notifications": "*",
        "kartik-v/yii2-tabs-x": "*",
        "2amigos/yii2-ckeditor-widget" : "^2.1@dev",
        "kartik-v/yii2-date-range": "*",
        "bedezign/yii2-audit": "*",
        "kartik-v/yii2-editable": "^1.7",
        "bizley/migration": "4.0-RC1",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-password": "dev-master",
        "kartik-v/yii2-builder": "dev-master",
        "kartik-v/yii2-helpers": "dev-master",
        "kartik-v/yii2-validators": "dev-master",
        "kartik-v/yii2-checkbox-x": "dev-master",
        "kartik-v/yii2-icons": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
